{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport Sidebar from '@/components/Sidebar.vue';\nexport default {\n  name: 'ArtPage',\n  components: {\n    Sidebar\n  },\n  data() {\n    return {\n      currentCategory: 'All',\n      categories: ['All', 'Paintings', 'Digital', 'Mixed Media'],\n      artworks: [{\n        title: '抽象画1',\n        technique: 'アクリル画',\n        year: '2024',\n        category: 'Paintings',\n        image: require('@/assets/placeholder.jpg')\n      }, {\n        title: 'デジタルアート1',\n        technique: 'デジタルペインティング',\n        year: '2023',\n        category: 'Digital',\n        image: require('@/assets/placeholder.jpg')\n      }, {\n        title: 'ミクストメディア作品1',\n        technique: '複合技法',\n        year: '2024',\n        category: 'Mixed Media',\n        image: require('@/assets/placeholder.jpg')\n      }]\n    };\n  },\n  computed: {\n    filteredArtworks() {\n      if (this.currentCategory === 'All') {\n        return this.artworks;\n      }\n      return this.artworks.filter(art => art.category === this.currentCategory);\n    }\n  }\n};","map":{"version":3,"names":["Sidebar","name","components","data","currentCategory","categories","artworks","title","technique","year","category","image","require","computed","filteredArtworks","filter","art"],"sources":["/Users/yusui/GitHub/Mono/site2/src/views/Art.vue"],"sourcesContent":["<template>\n  <div class=\"art-page\">\n    <h1>Art Gallery</h1>\n    <div class=\"art-categories\">\n      <div class=\"category-tabs\">\n        <button \n          v-for=\"category in categories\" \n          :key=\"category\"\n          @click=\"currentCategory = category\"\n          :class=\"{ active: currentCategory === category }\"\n        >\n          {{ category }}\n        </button>\n      </div>\n    </div>\n    <div class=\"art-grid\">\n      <div class=\"art-item\" v-for=\"(artwork, index) in filteredArtworks\" :key=\"index\">